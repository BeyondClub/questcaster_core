generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model questcaster_allowlist {
    allowlist_id String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    email        String?
    created_at   DateTime? @default(now()) @db.Timestamp(6)
}
